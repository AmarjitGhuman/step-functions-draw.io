{
  "extends": ["eslint:recommended"],
  "plugins": [],
  "parserOptions": {},
  "env": {"browser": true, "es6": true},
  "globals": {
    "mxscript": false,
    "Draw": false,
    "Editor": false,
    "mxUtils": false,
    "mxEvent": false,
    "mxCellState": false,
    "mxConnectionHandler": false,
    "mxGraph": false,
    "mxObjectCodec": false,
    "mxVertexHandler": false,
    "mxEdgeHandler": false,
    "mxCell": false,
    "mxGeometry": false,
    "mxPoint": false,
    "mxResources": false,
    "mxEventObject": false,
    "mxHierarchicalLayout": false,
    "mxConstants": false,
    "mxMorphing": false,
    "mxForm": false,
    "mxCodecRegistry": false,
    "EditDataDialog": true,
    "mxCodec": false,
    "mxClient": false,
    "AWS": false,
    "__listStateMachines": false,
    "__describeStateMachine": false,
    "__updateAWSconfig": false,
    "__deployStateMachine": false,
    "jsyaml": false,
    "jsep": false
  },
  "rules": {
    "indent": ["error", 2],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 1 }],
    "semi": "error",
    "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
    "space-before-blocks": ["error", "always"],
    "space-before-function-paren": ["error", "always"],
    "camelcase": "error"
  }
}
